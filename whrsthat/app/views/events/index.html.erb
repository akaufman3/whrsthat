<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Caption</th>
      <th>Time at</th>
      <th>Event img url</th>
      <th>Lng</th>
      <th>Lat</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% binding.pry %>
  <% if @events == nil %>
    <p> NO EVENTS </p>
  <% else %>
    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.title %></td>
          <td><%= event.caption %></td>
          <td><%= event.time_at %></td>
          <td><%= event.event_img_url %></td>
          <td><%= event.lng %></td>
          <td><%= event.lat %></td>
          <td><%= link_to 'Show', event %></td>
          <td><%= link_to 'Edit', edit_event_path(event) %></td>
          <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
    <% end %>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
